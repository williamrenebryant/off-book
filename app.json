{
  "expo": {
    "name": "Off Book",
    "slug": "off-book",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "offbook",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5EFE6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.offbook.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Off Book uses your microphone so you can practice your lines out loud.",
        "NSSpeechRecognitionUsageDescription": "Off Book listens to your lines and gives you feedback on accuracy.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F5EFE6"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.offbook.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-speech-recognition",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#F5EFE6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3a3f8c62-bf78-462c-a99b-719852f9d5d2"
      }
    }
  }
}
